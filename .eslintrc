{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "react/no-array-index-key": "off",
    "arrow-body-style": "off",
    "no-else-return": "off",
    "semi": 0,
    "react/jsx-filename-extension": [
      1,
      { "extensions":
          [".js", ".jsx"]
      }
    ]
  }
}